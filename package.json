{
  "name": "surveyjs-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:noturbo": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "migrate": "dotenv -e .env.local npx prisma migrate dev",
    "migrate:create": "dotenv -e .env.local npx prisma migrate dev --create-only",
    "migrate:deploy": "dotenv -e .env.local npx prisma migrate deploy",
    "studio": "dotenv -e .env.local npx prisma studio",
    "db:dev:rm": "docker compose --env-file .env.local -p oogned rm db -s -f -v",
    "db:dev:up": "docker compose --env-file .env.local -p oogned up db pgadmin -d",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up"
  },
  "dependencies": {
    "@auth/pg-adapter": "^1.2.0",
    "@auth/prisma-adapter": "^2.2.0",
