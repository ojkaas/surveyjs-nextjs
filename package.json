{
  "name": "oogned",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:noturbo": "next dev",
    "build": "next build",
    "start": "next start",
    "start:test": "cross-env NODE_ENV=test next dev -p 3001",
    "prestart:test": "npm run db:test:restart",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "migrate": "dotenv -e .env.local npx prisma migrate dev",
    "migrate:create": "dotenv -e .env.local npx prisma migrate dev --create-only",
    "migrate:deploy": "dotenv -e .env.local npx prisma migrate deploy",
    "prisma:reset": "cross-env NODE_ENV=test prisma migrate reset --force",
    "studio": "dotenv -e .env.local npx prisma studio",
    "db:dev:rm": "docker compose --env-file .env.local -p oogned rm db -s -f -v",
    "db:dev:up": "docker compose --env-file .env.local -p oogned up db pgadmin mailpit -d",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up",
    "test": "playwright test",
    "db:test:rm": "docker compose --env-file .env.test -p oogned rm test-db mailhog minio createbuckets -s -f -v",
    "db:test:up": "docker compose --env-file .env.test -p oogned up test-db mailhog minio createbuckets -d",
    "db:test:deploy": "dotenv -e .env.test npx prisma migrate deploy",
    "db:test:restart": "npm run db:test:rm && npm run db:test:up && npm run db:test:deploy && npm run db:test:seed",
    "db:test:seed": "dotenv -e .env.test npx prisma db seed",
    "docker:e2e:start": "npm run prisma:reset && start-server-and-test 'npm start' 3001 'npm run test:e2e'",
    "docker:e2e:build": "docker compose --env-file .env.test -f docker-compose.e2e.yml build",
    "docker:e2e:clean": "docker compose --env-file .env.test -f docker-compose.e2e.yml rm",
    "docker:e2e:ci": "docker compose --env-file .env.test -f docker-compose.e2e.yml run e2e",
    "test:e2e": "npx playwright test",
    "nvm:setversion": "node -v > .nvmrc",
    "upgrade-deps": "npm-check-updates -i"
  },
  "dependencies": {
    "@auth/pg-adapter": "^1.7.4",
    "@auth/prisma-adapter": "^2.7.4",
    "@hookform/resolvers": "^4.1.2",
    "@next/bundle-analyzer": "15.1.7",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@radix-ui/react-visually-hidden": "^1.1.2",
    "@tanstack/react-table": "^8.21.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "framer-motion": "^12.4.7",
    "handlebars": "^4.7.8",
    "luxon": "^3.5.0",
    "minio": "^8.0.4",
    "next": "15.1.7",
    "next-auth": "^4.24.11",
    "next-cloudimage-responsive": "^1.2.6",
    "next-safe-action": "^7.10.4",
    "nextjs-toploader": "^3.7.15",
    "nodemailer": "^6.10.0",
    "prisma": "^5.22.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-turnstile": "^1.1.4",
    "sonner": "^1.7.0",
    "survey-core": "^1.12.25",
    "survey-creator-core": "^1.12.25",
    "survey-creator-react": "^1.12.25",
    "survey-react-ui": "^1.12.25",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.11",
    "@types/react": "npm:types-react@19.0.0-alpha.3",
    "@types/react-dom": "npm:types-react-dom@19.0.0-alpha.3",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-config-next": "15.1.7",
    "eslint-plugin-prettier": "^5.2.3",
    "mailhog-awesome": "^0.1.4",
    "playwright": "^1.50.1",
    "postcss": "^8",
    "prettier": "^3.5.2",
    "prettier-eslint": "^16.3.0",
    "start-server-and-test": "^2.0.10",
    "tailwindcss": "^3.4.15",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "zod": "^3.24.2"
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-alpha.3",
    "@types/react-dom": "npm:types-react-dom@19.0.0-alpha.3"
  }
}
